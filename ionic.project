{
  "name": "Soru Mania",
  "app_id": "",
  "gulpStartupTasks": [
    "sass",
    "watch"
  ],
  "watchPatterns": [
    "www/**/*",
    "!www/lib/**/*"
  ],
  "browsers": [
    {
      "platform": "android",
      "browser": "crosswalk",
      "version": "12.41.296.5"
    }
  ],
  "proxies": [
    {
      "path": "/api",
      "proxyUrl": "http://localhost:8080/api"
    },
    {
      "path": "/oauth",
      "proxyUrl": "http://localhost:8080/oauth"
    }
  ]
}
